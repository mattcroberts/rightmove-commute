{
  "manifest_version": 2,
  "name": "Rightmove Commute",
  "version": "0.1.2",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://www.rightmove.co.uk/*"],
      "js": ["index.js"]
    }
  ]
}
